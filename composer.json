{
    "name": "eleven59/backpack-shop",
    "description": "Webshop crud models and functionality for Backpack for Laravel. Love it.",
    "license": "MIT",
    "authors": [
        {
            "name": "eleven59.nl",
            "email": "info@eleven59.nl",
            "homepage": "https://eleven59.nl"
        }
    ],
    "homepage": "https://github.com/eleven59/backpack-shop",
    "keywords": [
        "Laravel", "Backpack", "Backpack for Laravel", "Backpack Addon", "BackpackShop"
    ],
    "require": {
        "backpack/crud": "^5.0.0",
        "backpack/pro": "^1.0.0",
        "barryvdh/laravel-dompdf": "^2.0",
        "illuminate/support": "~8|~9",
        "eleven59/backpack-image-traits": "^1.0",
        "pragmarx/countries-laravel": "^0.7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Eleven59\\BackpackShop\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eleven59\\BackpackShop\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eleven59\\BackpackShop\\AddonServiceProvider"
            ]
        }
    }
}
